{
  "name": "fullstack-starter",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.2.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "db:generate": "turbo db:generate",
    "db:migrate": "turbo db:migrate"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/bun": "^1.2.19",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
